<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="signUpRoot" prefHeight="580.0" prefWidth="450.0" style="-fx-background-color: rgb(255,255,255);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UserInterfaceController">
	<children>
		<ImageView fitHeight="90.0" fitWidth="350.0" layoutX="53.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
        	<image>
            	<Image url="https://i.ibb.co/WpkkLh2/logga.png" />
         	</image>
      	</ImageView>
      	<TextField fx:id="usernameSignUp" layoutX="50.0" layoutY="175.0" prefHeight="40.0" prefWidth="350.0" promptText="Username" />
     	<Label fx:id="usernameError" layoutX="62.0" layoutY="220.0" prefHeight="18.0" prefWidth="326.0" text="" textFill="#cc1d1d">
        	<font>
            	<Font name="Helvetica Neue Bold" size="14.0" />
         	</font>
      	</Label>
		<PasswordField fx:id="passwordSignUp" layoutX="50.0" layoutY="250.0" prefHeight="40.0" prefWidth="350.0" promptText="Password" />
    	<Label fx:id="passwordError" layoutX="62.0" layoutY="295.0" prefHeight="18.0" prefWidth="326.0" text="" textFill="#cc1d1d">
        	<font>
	            <Font name="Helvetica Neue Bold" size="14.0" />
         	</font>
      	</Label>
     	<TextField fx:id="emailSignUp" layoutX="50.0" layoutY="325.0" prefHeight="40.0" prefWidth="350.0" promptText="Email" />
      	<Label fx:id="emailError" layoutX="62.0" layoutY="370.0" prefHeight="18.0" prefWidth="326.0" text="" textFill="#cc1d1d">
        	<font>
            	<Font name="Helvetica Neue Bold" size="14.0" />
         	</font>
      	</Label>

      	<Button fx:id="signUp" layoutX="50.0" layoutY="475.0" mnemonicParsing="false" onAction="#signUpButton" prefHeight="50.0" prefWidth="350.0" style="-fx-background-color: rgb(90, 51, 103);" text="Sign up" textFill="WHITE">
        	<font>
            	<Font name="Helvetica Neue Bold" size="16.0" />
         	</font>
      	</Button>
	    <Button fx:id="cancelSignUp" layoutX="82.0" layoutY="536.0" mnemonicParsing="false" onAction="#cancelSignUpButton" style="-fx-background-color: rgb(255,255,255);" text="Return" textFill="#5a3367">
        	<font>
            	<Font name="Helvetica Neue Bold" size="13.0" />
         	</font>
      	</Button>
   	</children>
</AnchorPane>
